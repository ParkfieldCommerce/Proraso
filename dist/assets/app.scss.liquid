  }
  .About__text-container--inner{
    width: auto;
    margin-left: auto;
    margin-right: auto;
  }
}
.ShopByCategory__heading{
  text-transform: uppercase;
  text-align: center;
  margin-bottom: 40px;
}
.ShopByCategory__collections{
  @include flex();
  flex-flow: row wrap;
  @include justify-content(space-between);
}
.ShopByCategory__collection{
  text-align: center;
  &:hover{
    .ShopByCategory__link{
      color: $font-color;
    }
    .ShopByCategory__image{
      background-color: #c69f51cc;
    }
  }
}
.ShopByCategory__image{
  height: 138px;
  width: 138px;
  margin-left: auto;
  margin-right: auto;
  border-radius: 138px;
  background-color: #c69f514d;
  @include flex();
  @include align-items(center);
  @include justify-content(center);
  transition: background-color .3s;
  margin-bottom: 20px;
}
@include maxQuery(980px){
  .ShopByCategory__collection{
    width: 33.333%;
  }
}
@include maxQuery(550px){
  .ShopByCategory__image{
    width: 80px;
    height: 80px;
    img{
      max-width: 50%;
      max-height: 50%;
    }
  }
}
//Collection
.CollectionProduct{
  text-align: center;
  &:hover{
    .CollectionProduct__image-container{
      @include align-items(flex-start);
    }
    .CollectionProduct__overlay{
      opacity: 1;
    }
  }
}
.CollectionProduct__image-container{
  background: $bg-gray;
  @include flex();
  @include align-items(flex-end);
  position: relative;
}
.CollectionProduct__heading{
  @include din(2.4rem);
  font-style: normal;
  line-height: 1.125;
  display: block;
  margin-top: 14px;
}
.CollectionProduct__price--sale{
  color: $disabled;
  margin-right: 10px;
}
.CollectionProduct__prices{
  display: block;
  margin-top: 10px;
}
.CollectionProduct__button-text{
  position: relative;
  .fas{
    position: absolute;
    top: 50%;
    @include prefix(transform, translateY(-50%));
    font-size: 15px;
    right: -20px;
  }
}
.CollectionProduct__overlay{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,.3);
  transition: opacity .3s;
  opacity: 0;
  @include maxQuery(800px){
    display: none;
  }
}
.CollectionProduct__overlay-button{
  position: absolute;
  left: 50%;
  @include prefix(transform, translateX(-50%));
  bottom: 30px;
  border-color: white;
  color: white;
  font-size: 2.4rem;
  padding-top: 10px;
  padding-bottom: 10px;
  &:hover{
    color: white;
    border-color: white;
  }
}
.CollectionProduct .Quickview__info{
  display: none;
}
.Quickview__popup .popup-content{
  background-color: $bg-light;
  width: 800px;
  max-width: 85%;
  padding: 0;
}

.Quickview__header{
  background: $bg-dark;
  position: relative;
}
.Quickview__header__heading{
  color: white;
  text-transform: uppercase;
  margin-bottom: 0;
  text-align: center;
  font-size: 2.4rem;
  padding-top: 15px;
  padding-bottom: 15px;
}
.Quickview__close{
  position: absolute;
  color: white;
  top: 50%;
  @include prefix(transform, translateY(-50%));
  right: 20px;
  font-size: 1.5rem;
}
.Quickview__popup .Quickview__info{
  @include flex();
  @include align-items(center);
  padding: 40px;
}
.Quickview__image{
  width: 50%;
}
.Quickview__content{
  width: 50%;
}
.Quickview__heading{
  margin-bottom: 15px;
}
.Quickview__prices{
  font-size: 2.4rem;
  margin-bottom: 20px;
  display: inline-block;
}
.Quickview__price--sale{
  color: $disabled;
}
.Quickview__quantity{
  margin-bottom: 20px;
}
.Quickview__actions{
  @include flex();
  @include align-items(center);
}
.Quickview__add{
  margin: 0;
  width: 160px;
  margin-right: 20px;
}
.Quickview__link{
  text-transform: uppercase;
}
